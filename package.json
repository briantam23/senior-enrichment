  "homepage": "https://senior-enrichment-btam.herokuapp.com",
  "version": "1.0.0",
  "description": "A Single Page App created with React, Redux, & Express, along with multiple Testing libraries (Enzyme, Mocha, Chai, Sinon, SuperTest).",
  "repository": {
    "type": "git",
    "url": "https://github.com/briantam23/senior-enrichment.git"
  },
  "main": "server/index.js",
  "scripts": {
    "start": "node ./server/index.js",
    "postinstall": "npm run webpack",
    "webpack": "./node_modules/webpack/bin/webpack.js",
    "build": "npm run webpack -- --watch --mode=development",
    "start:dev": "npm run build & DATABASE_URL=postgres://localhost/senior-enrichment nodemon server/index.js --ignore dist --ignore src",
    "test": "mocha ./test/*.test.js --watch --require babel-polyfill --compilers js:babel-register"
  },
  "keywords": [
    "react",
    "redux",
    "nodejs",
    "enzyme",
    "mocha",
    "postgres"
  ],
  "license": "ISC",
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-3"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "chalk": "^2.4.1",
    "express": "^4.16.3",
    "pg": "^7.4.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-loading": "^2.0.3",
